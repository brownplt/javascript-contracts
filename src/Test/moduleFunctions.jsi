{

  function isNumber(x) {
    return typeof(x) == "number";
  }
  
  function isString(x) {
    return typeof(x) == "string";
  }

  function isBool(x) { // perhaps overly strict, but great for testing
    return x === true || x === false;
  }

  function any(x) { return true; }

  function isArray(x) { return x instanceof Array; }
}

